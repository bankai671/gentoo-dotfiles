syntax on
filetype on
filetype indent on
set cindent
set nocompatible
set number
set shiftwidth=4
set tabstop=4
set nobackup
set wildmenu
set mouse=a
set clipboard^=unnamed,unnamedplus
set nobackup
set background=dark
set hlsearch
set incsearch

call plug#begin()

Plug 'sheerun/vim-polyglot'

call plug#end()
